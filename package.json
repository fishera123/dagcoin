    "bitcore-mnemonic": "~1.0.0",
    "browser-request": "^0.3.3",
    "byteballcore": "^0.2.54",
    "chokidar": "^1.7.0",
    "core-js": "^2.4.1",
    "dagcoin-core": "^0.6.2",
    "foundation-sites": "^6.4.3",
    "jszip": "^3.1.3",
    "lodash": "^4.17.4",
    "preconditions": "^1.0.8",
