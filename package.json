{
  "name": "dagcoin",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.html",
  "scripts": {
    "start-web": "webpack-dev-server --inline --progress --port 8080",
    "start": "concurrently \"npm run webpack:w\" \"npm run nw\"",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail && npm run nw",
    "postinstall": "typings install",
    "nw": "nw .",
    "webpack:w": "webpack --display-error-details -w"
  },
  "webkit": {
    "page-cache": false,
    "java": false,
    "plugin": false,
    "partitions": [
      {
        "name": "assets",
        "accessible_resources": [
          "/assets/**"
        ]
      }
    ]
  },
  "window": {
    "title": "Angular2 NWJS webpack",
    "width": 600,
    "height": 600,
    "position": "center",
    "min_width": 100,
    "min_height": 100
  },
  "dependencies": {
